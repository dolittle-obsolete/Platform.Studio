{
  "name": "@dolittle/studio-login",
  "description": "A tool for logging in to the Dolittle studio",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://dolittle.io",
  "author": "Dolittle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dolittle-runtime/runtime.git"
  },
  "bugs": {
    "url": "https://github.com/dolittle-runtime/runtime/issues"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "keywords": [
    "dolittle",
